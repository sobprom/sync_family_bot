<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://www.jooq.org">
    <jdbc>
        <driver>org.sqlite.JDBC</driver>
        <url>jdbc:sqlite:sync_family.db</url>
    </jdbc>
    <generator>
        <database>
            <name>org.jooq.meta.sqlite.SQLiteDatabase</name>
            <includes>.*</includes>
        </database>
        <target>
            <packageName>ru.syncfamily.jooq</packageName>
            <directory>target/generated-sources/jooq</directory>
        </target>
    </generator>
</configuration>